<!--
	HSTS Preloaded:
		- mega.nz    (removed from ruleset)
		- mega.co.nz (doesn't have `include_subdomains`)
-->
<ruleset name="Mega">
	<target host="*.mega.co.nz" />
	<test url="http://eu.static.mega.co.nz/" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"	to="https:" />
</ruleset>
